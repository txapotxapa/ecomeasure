{
	"appId": "com.ecomeasure.app",
	"appName": "EcoMeasure",
	"webDir": "dist/public",
	"server": {
		"androidScheme": "https"
	},
	"android": {
		"allowMixedContent": true,
		"captureInput": true,
		"webContentsDebuggingEnabled": true
	},
	"plugins": {
		"Camera": {
			"permissions": [
				"camera",
				"photos"
			]
		},
		"Geolocation": {
			"permissions": [
				"location"
			]
		},
		"Filesystem": {
			"permissions": [
				"storage"
			]
		},
		"App": {
			"handleBackButton": false
		}
	}
}
